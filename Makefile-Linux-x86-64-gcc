KD_MAKEFILE = Makefile-Linux-x86-64-gcc

all:
	cd ../coresys/make;  make -j4 -f $(KD_MAKEFILE) all
	cd ../apps/make;  make -j4 -f $(KD_MAKEFILE) all_but_hyperdoc
	cd ../managed/make;  make -j4 -f $(KD_MAKEFILE) all

clean:
	cd ../coresys/make;  make -j4 -f $(KD_MAKEFILE) clean
	cd ../apps/make;  make -j4 -f $(KD_MAKEFILE) clean
	cd ../managed/make;  make -j4 -f $(KD_MAKEFILE) clean
